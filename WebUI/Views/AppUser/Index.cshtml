@model  IEnumerable<WebUI.Models.AppUserViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Gender</th>
        <th>DOB</th>
        <th>Profile Image</th>
    </tr>

    <tr>
        @foreach (var item in Model)
        {
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Phone</td>
            <td>@item.Email</td>
            <td>@item.Gender</td>
            <td>@item.DOB</td>
            <td>@item.ProfileImage</td>
        }
    </tr>

</table>

